package training;

public class increment12 {
int c;
static int e;
increment12(){
	System.out.println(++c);
	System.out.println(++e);
	
	
}
public static void main(String[] args) {
	int a=34;
	int b=21;
	new increment12().c-=a++ + ++b;
	int d=--a+--b+new increment12().c--;
	e=a+ +b+ +new increment12().c-d++;
	int f=-a+b--+-new increment12().c-d++;
	int sum	=a+b+new increment12().c+d+e+f;
	System.out.println("Sum:"+sum);
	
}
