package training;

public class vote {
	void checkEligibility(String name,int age) {
		if(age>=18) {
			System.out.println(name+" Eligible to vote");
		}
		else {
			System.out.println(name+"not Eligible to vote");
		}
	}
	public static void main(String[] args) {
		new vote().checkEligibility("Vinathi", 35);
	}

}
